<!DOCTYPE html>
<html lang="en">
<head>
    <script type="text/javascript">
        function table()
        {
            var n=parseInt(document.getElementById("myid1").value);
            for(var i=1;i<=10;i++)
            {
                var t=(n*i);
                document.getElementById("myid2").innerHTML=(n+"*"+i)=t;
            }
        }
    </script>
    
    <title>Create Table</title>
</head>
<body>
    <table>
        <tr>
            <td><input type="text" id="myid1" placeholder="Enter Number"></td><br>
            <td><button onclick="table()" type="button">Create Table</button></td>
        </tr>
    </table>
    <div id="myid2">Print Table Here</div>
    
</body>
</html>
